//             PROGRAMS TO  Prime or not:
#include<iostream>
using name space std;
int main(){
    int n;
    cout<<"Enter the number to check whether its Prime or not";
    cin>>n;
    int i=2;
    while (i<n)
    {
        if (n%i==0)
        {
            cout<<"Given number is not Prime"<<endl;
            return 0;
        }
        i=i+1; 
    }
    return 0;
    cout<<""
}
